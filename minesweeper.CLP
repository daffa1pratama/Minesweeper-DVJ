(defrule explore-1
	(b_size ?size)
	(user ?x ?y 0)
	(test (< (+ ?x 1) ?size))
	(test (>= (- ?x 1) 0))
	(test (>= (- ?y 1) 0))
	(test (< (+ ?y 1) ?size))
	(tile ?z1 ?y ?val1)
	(tile ?x ?z2 ?val2)
	(tile ?z3 ?y ?val3)
	(tile ?x ?z4 ?val4)
	(tile ?z1 ?z2 ?val5)
	(tile ?z1 ?z4 ?val6)	
	(tile ?z3 ?z2 ?val7)
	(tile ?z3 ?z4 ?val8)
	(test (= ?z1 (+ ?x 1)))
	(test (= ?z2 (+ ?y 1)))
	(test (= ?z3 (- ?x 1)))
	(test (= ?z4 (- ?y 1)))
	=>
	(assert (batasrule baru))
	(assert (user (+ ?x 1) ?y ?val1))
	(assert (user ?x (+ ?y 1) ?val2))
	(assert (user (- ?x 1) ?y ?val3))
	(assert (user ?x (- ?y 1) ?val4))
	(assert (user (+ ?x 1) (+ ?y 1) ?val5))
	(assert (user (+ ?x 1) (- ?y 1) ?val6))	
	(assert (user (- ?x 1) (+ ?y 1) ?val7))
	(assert (user (- ?x 1) (- ?y 1) ?val8))
)

(defrule explore-2
	(user 0 0 0)
